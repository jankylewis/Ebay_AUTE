<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Onboarding experience" parallel="true">
    <test name="Log-in flows 1">
        <classes>
            <class name="se.spo.gui.onboardingExperience.LogInTest">
                <methods>
                    <include name="logInSuccessfully"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Log-in flows 2">
        <classes>
            <class name="se.spo.gui.onboardingExperience.LogInTest">
                <methods>
                    <include name="logInUnsuccessfullyWithBadCredentials"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>