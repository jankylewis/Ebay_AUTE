<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Authentication Tests" parallel="tests" thread-count="2">
    <test name="Sign Up 1">
        <classes>
            <class name="se.spo.gui.onboardingExperience.SignUpTest">
                <methods>
                    <include name="signUpUnsuccessfullyWithInvalidPrefix"></include>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Sign Up 2">
        <classes>
            <class name="se.spo.gui.onboardingExperience.SignUpTest">
                <methods>
                    <include name="signUpUnsuccessfullyWithInvalidSuffix"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>